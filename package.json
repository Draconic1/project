{
  "name": "bmstu-example",
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "tscompile": "tsc",
    "eslint": "npx eslint --ext js,mjs,jsx,ts,tsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "npx stylelint **/*.css",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "lint": "npm run tscompile && npm run eslint && npm run stylelint && npm run prettier",
    "lint:fix": "npm run tscompile && npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "generate:api": "cross-env BE_URL='http://127.0.0.1:8000/api/swagger/?format=openapi' node scripts/generate-api.mjs",
    "update:deps": "npx npm-check -u",
    "generate-backend-types": "swagger-typescript-api -p http://127.0.0.1:8000/api/swagger/?format=openapi --no-client -o ./types/autogenerated -n backend.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.16",
    "@mui/joy": "^5.0.0-beta.14",
    "@mui/material": "^5.14.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@vitejs/plugin-react": "^4.1.0",
    "axios": "^1.5.0",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^14.0.1",
    "npm-check": "^6.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.2.2",
    "utility-types": "^3.10.0",
    "vite": "^4.4.9"
  }
}
